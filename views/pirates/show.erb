<h1>Pirate</h1>
  <div class="priate">
    <h3>Priate Name: <%= @pirate.name %></h3>
    <h3>Priate Weight: <%= @pirate.weight %></h3>
    <h3>Priate Height: <%= @pirate.height %></h3>
  </div>

<h2>The Pirate's Ships</h2>
  <%= @ships.each do |ship| %>
    <div class="ship">
      <h3>Ship Name: <%= @ship.name %></h3>
      <h3>Ship Type: <%= @ship.type %></h3>
      <h3>Ship Booty: <%= @ship.booty %></h3>
    </div>
  <%= end %> 
